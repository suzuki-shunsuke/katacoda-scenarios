{
  "automerge": true,
  "extends": [
    "config:base",
    "github>aquaproj/aqua-renovate-config:file(aqua-quick-start/assets/.*\\.yaml)"
  ],
  "packageRules": [
    {
      "enabled": false,
      "matchPackagePatterns": [".*"]
    },
    {
      "enabled": true,
      "matchPackageNames": ["aquaproj/aqua-registry"]
    }
  ]
}
